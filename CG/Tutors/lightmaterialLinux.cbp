<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Light Material" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/exampleLinux" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin" />
				<Option object_output="../bin/_obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-Wno-unused-function" />
			<Add option="-fexceptions" />
			<Add directory="../include" />
			<Add directory="../bin" />
		</Compiler>
		<Linker>
			<Add library="glut" />
			<Add library="GL" />
			<Add library="GLU" />
			<Add library="z" />
		</Linker>
		<Unit filename="glm.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="glm.h" />
		<Unit filename="lightmaterial.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="materials.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
