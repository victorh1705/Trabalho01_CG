<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Texture" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/texture" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin" />
				<Option object_output="../bin/_obj" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-Wno-unused-function" />
			<Add option="-fexceptions" />
			<Add option="-Wno-write-strings" />
			<Add directory="../include" />
			<Add directory="../include/png" />
			<Add directory="../include/zlib" />
		</Compiler>
		<Linker>
			<Add library="libpng.a" />
			<Add library="libz.a" />
			<Add library="libglui32.a" />
			<Add library="libopengl32.a" />
			<Add library="libglu32.a" />
			<Add library="libfreeglut.a" />
			<Add library="libglew32.a" />
			<Add library="libglew32.dll.a" />
			<Add directory="../lib" />
		</Linker>
		<Unit filename="glm.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="glm.h" />
		<Unit filename="texture.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
